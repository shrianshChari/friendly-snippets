{
  "text": {
    "prefix": "text",
    "body": "\\text{$0}",
    "description": "Insert plain text into equation"
  },
  "alignrow": {
    "prefix": "alignrow",
    "body": "&= $0 && \\text{$1} \\\\",
    "description": "Insert new row in align"
  },
  "alignrow2": {
    "prefix": "alr",
    "body": "&= $0 && \\text{$1} \\\\\\\\",
    "description": "Insert new row in align"
  },
  "choose": {
    "prefix": "choose",
    "body": "{$0 \\choose $1}",
    "description": "n \\choose k"
  }
}
